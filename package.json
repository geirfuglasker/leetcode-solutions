{
  "name": "leetcode-solutions",
  "version": "0.0.0",
  "type": "module",
  "jest": {
    "verbose": true,
    "testMatch": ["**/__tests__/*.+(ts|tsx|js)"],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "scripts": {
    "test:subdomainVisits": "npx jest -t subdomainVisits",
    "test:numUniqueEmails": "npx jest -t numUniqueEmails",
    "test:uniqueMorseRepresentations": "npx jest -t uniqueMorseRepresentations",
    "test:isAlienSorted": "npx jest -t isAlienSorted",
    "test:all": "npx jest"
  },
  "dependencies": {
    "@types/jest": "^27.0.2",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.3.1",
    "typescript": "^4.4.4"
  }
}
